<div>
    <h3>Your Public Posts</h3>
    {{#if posts.length}}
        {{#each posts as |post|}}
            <div>
                <a href="/dashboard/edit/{{post.id}}">{{post.postTitle}}</a>
            </div>
        {{/each}}
    {{/if}}
</div>

<button><a href="/dashboard/new">Add New Post</a></button>

